{
  "name": "tp-devops-be",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run nodemon",
    "nodemon": "nodemon -L --exec \"node --inspect=0.0.0.0:9229 --import ./loader.js src/server.ts\"",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prepush": "npm run test",
    "pretest": "npm run lint",
    "start": "cross-env NODE_ENV=production node --import ./loader.js src/server.ts",
    "test": "cross-env NODE_ENV=testing jest --coverage",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test-ci": "jest --coverage --forceExit"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.3",
    "nodemailer": "^7.0.5",
    "payment": "^2.4.7",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.8.1",
    "sequelize": "^6.37.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@goldstack/nodemonx": "0.4.25",
    "@goldstack/utils-typescript-references": "0.3.15",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1"
  },
  "overrides": {
    "eslint": "^9.33.0"
  }
}
